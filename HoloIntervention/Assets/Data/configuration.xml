<?xml version="1.0" encoding="utf-8"?>
<HoloIntervention>
  <!-- HMD coordinate frame is the Hololens frame -->
  <Tools>
    <Tool 
      Model="hemisphere_stylus" 
      Transform="StylusTipToHMD"
      LerpRate="12.0"
     />
  </Tools>
  <CoordinateDefinitions>
  </CoordinateDefinitions>

  <SliceRendering From="Image" To="HMD" />
  <VolumeRendering From="Volume" To="HMD" />

</HoloIntervention>